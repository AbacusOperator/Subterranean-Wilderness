{
	"multipart":
	[
		{
			"apply": { "model": "block/ice" }
		},
		{
			"when": { "up": false, "down": false, "facing": "up" },
			"apply": { "model": "subwild:block/medium_granite_stalagmite" }
		},
		{
			"when": { "up": false, "down": false, "facing": "down" },
			"apply": { "model": "subwild:block/medium_granite_stalagmite", "x": 180 }
		},
		{
			"when": { "up": false, "down": true, "facing": "up" },
			"apply": { "model": "subwild:block/small_granite_stalagmite" }
		},
		{
			"when": { "up": true, "down": false, "facing": "down" },
			"apply": { "model": "subwild:block/small_granite_stalagmite", "x": 180 }
		},
		{
			"when": { "up": true, "down": false, "facing": "up" },
			"apply": { "model": "subwild:block/large_granite_stalagmite" }
		},
		{
			"when": { "up": false, "down": true, "facing": "down" },
			"apply": { "model": "subwild:block/large_granite_stalagmite", "x": 180 }
		},
		{
			"when": { "up": true, "down": true, "facing": "up" },
			"apply": { "model": "subwild:block/granite_column" }
		},
		{
			"when": { "up": true, "down": true, "facing": "down" },
			"apply": { "model": "subwild:block/granite_column" }
		}
	]
}